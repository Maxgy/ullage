print 1111 if 100 == 100 else 0000 # => 1111
print 1234 if 567 == 890 else -900 # => -900
print 90-1 if 4/2 == 1+1 else 22-9 # => 89

print 1 unless 2 == 3 else 4 # => 1
print 0 unless 0 == 0 else 1 # => 1

print 1 if 2 != 3 else 4 # => 1
print 0 if 1 < 2 else 3 # => 0
print 0 if 1 > 2 else 3 # => 3

print 1 if !1 == 3 else 3 # => 3
print 1 if !(1 == 3) else 3 # => 1