// entry tokens
#[pub]
words -> Vec<String>
    = sp w:(word*) { w }

// Main tokens
word -> String
    = w:word_chars sp { w }

word_chars -> String
    = [a-zA-z]+ { match_str.to_string() }

// whitepsace handling
sp
    = (space_char / comment / newline)*
space_char
    = [ \t]
newline
    = [\n\r] / "\r\n"
comment
    = "//" [^\n]*